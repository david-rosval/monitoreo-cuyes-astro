---
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../../consts';

const urlActaDeConstancia = "/acta-de-constancia.pdf"

---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
		<style>
			main {
				width: 960px;
			}
			
		</style>
	</head>
	<body>
		<Header />
		<main>
      <h1>Entregables</h1>
      <ol>
        <li>
          <p>Acta de Constancia - Semana 3 (12/09/2024)</p>
          <iframe src={urlActaDeConstancia} width="100%" height="600px"></iframe>
        </li>
      </ol>
		</main>
		<Footer />
	</body>
</html>
